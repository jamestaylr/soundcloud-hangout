<?xml version="1.0" encoding="UTF-8" ?>
<Module>
	<ModulePrefs title="Soundcloud for Hangouts">
		<Require feature="dynamic-height" />
		<Require feature="rpc" />
		<Require feature="views" />
	</ModulePrefs>
	<Content type="html"><![CDATA[    
<script src="//plus.google.com/hangouts/_/api/v1/hangout.js"></script>
<script type="text/javascript" src="assets/base.js"></script>
<link rel="stylesheet" type="text/css" href="assets/base.css">

<body>
	<object type="image/svg+xml" data="assets/icon.svg"></object>
	<input type="text" size="40" onkeydown="if (event.keyCode == 13) { createEmbededSound(event); return false; }"/>
	<div id="playing" style="visibility: hidden"></div>
</body>
]]>
	</Content>
</Module>

